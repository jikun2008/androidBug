<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.testonactivityreslut"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="20" />

    <application
        android:name="com.example.testonactivityreslut.MainAppcation"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.testonactivityreslut.StandardActivity"
            android:launchMode="standard" >
        </activity>
        <activity
            android:name="com.example.testonactivityreslut.SingleTopActivity"
            android:launchMode="singleTop" >
        </activity>
        <activity
            android:name="com.example.testonactivityreslut.SingleTaskActivity"
            android:launchMode="singleTask"
            android:taskAffinity="com.test.show" >
        </activity>
        <activity
            android:name="com.example.testonactivityreslut.SingleInstanceActivity"
            android:launchMode="singleInstance" >
        </activity>
    </application>

</manifest>